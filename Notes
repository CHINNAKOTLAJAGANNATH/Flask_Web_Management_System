sqlite> .schema user
CREATE TABLE user (
        id INTEGER NOT NULL,
        name VARCHAR(100) NOT NULL,
        email VARCHAR(100) NOT NULL,
        username VARCHAR(100) NOT NULL,
        password VARCHAR(100) NOT NULL,
        PRIMARY KEY (id)
);


run:

pip install -r requirements.txt

python run.py  

